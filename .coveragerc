[run]
branch = True

omit =
    /kb/module/lib/ExpressionUtils/authclient.py
    /kb/module/lib/ExpressionUtils/baseclient.py
    /kb/module/lib/ExpressionUtils/ExpressionUtilsClient.py
    /kb/module/lib/ExpressionUtils/ExpressionUtilsServer.py
source = /kb/module/lib/ExpressionUtils


[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    # Have to re-enable the standard pragma
    pragma: no cover
    # Don't complain if tests don't hit defensive assertion code:
    raise NotImplementedError

show_missing = 1

